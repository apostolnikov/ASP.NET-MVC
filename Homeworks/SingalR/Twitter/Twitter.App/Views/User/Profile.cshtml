@using Twitter.App.Models.ViewModels.UsersViewModels
@using System.Web.UI.WebControls
@using Glimpse.Mvc.Html

@model UserProfileViewModel

@{
    ViewBag.Title = "Profile";
}
<br />
<div class="row">
    <div class="col-lg-3">
        <img src="data:image/jpeg;base64," alt="If you have Internet here you will see a donkey" style="max-width: 200px; max-height: 150px"/>
        <div class="row">
            <h3> @Model.Username</h3>
        </div>
        <div>
            @using (@Html.BeginForm("BecomeFollowing", "Notifications"))
            {
                @Html.TextBox("id", @Model.Username, new {hidden = true})
                <button class="btn btn-primary" type="submit">Follow</button>
            }
        </div>
    </div>
    <div class="col-lg-9">
        <ul class="nav-justified">
            <li>
                <div>
                    <b>Tweets</b>
                </div>
                <span class="badge">@Model.Tweets.Count</span>
            </li>
            <li>
                <div>
                    <b>Followers</b>
                </div>
                <span class="badge"> @Model.FollowersCount</span>
            </li>
            <li>
                <div>
                    <b>Following</b>
                </div>
                <span class="badge">@Model.FollowingsCount</span>
            </li>
            <li>
                <div>
                    <b>Favorities</b>
                </div>
                <span class="badge">@Model.FavoritiesCount</span>
            </li>
        </ul>
        <br/>
        <div class="row">
            @foreach (var @tweet in @Model.Tweets)
            {
                Html.RenderPartial("~/Views/Shared/_Tweet.cshtml", @tweet);
            }
        </div>
    </div>
</div>


